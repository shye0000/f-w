{
  "name": "fondations",
  "sideEffects": [
    "*.css"
  ],
  "version": "0.1.0",
  "description": "",
  "browserslist": [
    "defaults",
    "last 10 versions"
  ],
  "config": {
    "localePath": "./locale"
  },
  "repository": "https://gitlab.wcentric.com/jts/productivity/react/fondations-front",
  "scripts": {
    "test": "jest --colors",
    "update-snapshot": "jest --updateSnapshot",
    "coverage": "jest --colors --coverage",
    "doc": "esdoc",
    "i18n-add-locale": "lingui add-locale fr_FR ja_JP",
    "i18n-extract": "lingui extract --clean",
    "i18n-compile": "lingui compile && babel-node ./node_modules/wbc-components/lib/Translations/utils/customTranslations.js",
    "build-prod": "npm run i18n-compile && export NODE_ENV=production && export APP_ENV=prod && ./node_modules/.bin/webpack -p --profile --optimize-minimize",
    "build-demo": "npm run i18n-compile && export NODE_ENV=production && export APP_ENV=demo && ./node_modules/.bin/webpack -p --profile --optimize-minimize",
    "build-demo_client": "npm run i18n-compile && export NODE_ENV=production && export APP_ENV=demo_client && ./node_modules/.bin/webpack -p --profile --optimize-minimize",
    "build-moa": "npm run i18n-compile && export NODE_ENV=production && export APP_ENV=moa && ./node_modules/.bin/webpack -p --profile --optimize-minimize",
    "build-preprod": "npm run i18n-compile && export NODE_ENV=production && export APP_ENV=preprod && ./node_modules/.bin/webpack -p --profile --optimize-minimize",
    "build-test-prod": "npm run i18n-compile && export NODE_ENV=production && export APP_ENV=dev && ./node_modules/.bin/webpack -p --profile --optimize-minimize",
    "version-major": "npm version major -m \"Upgrade to %s\"",
    "version-minor": "npm version minor -m \"Upgrade to %s\"",
    "version-patch": "npm version patch -m \"Upgrade to %s\"",
    "start": "export NODE_ENV=production && ./node_modules/.bin/babel-node src/server.js",
    "dev": "npm run i18n-compile && export NODE_ENV=development && export APP_ENV=dev && ./node_modules/.bin/concurrently --kill-others \"./node_modules/.bin/nodemon src/server.js --exec ./node_modules/.bin/babel-node\" \"./node_modules/.bin/webpack-dev-server\""
  },
  "jest": {
    "setupFiles": [
      "./setupTests.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      ".*\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "globals": {
      "AppConfig": {
        "apiEntryPoint": "test"
      }
    }
  },
  "lingui": {
    "format": "minimal",
    "localeDir": "<rootDir>/locale",
    "srcPathDirs": [
      "<rootDir>/src"
    ]
  },
  "keywords": [],
  "author": "Shuo LI",
  "license": "ISC",
  "devDependencies": {
    "antd": "^3.8.0",
